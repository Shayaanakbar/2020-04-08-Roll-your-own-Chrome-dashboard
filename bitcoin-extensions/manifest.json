{
	"manifest_version": 2,
    "name": "Bitcoin Chrome Extension",
    "version": "1.0",
	"description": "This is the description for your first chrome extension",
	"content_scripts": [
		{
			"matches": ["https://www.google.com/*"],
			"css": ["styles.css"],
			"js": ["scripts.js"]
		}
	],
	"browser_action": {
 		"default_icon": "bitcoin.png",
 		"default_popup": "bitcoin.html"
 	}
}